{
  "compilerOptions": {
    "strict": false,
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "preserve",
    "esModuleInterop": true,
    "sourceMap": true,
    "allowJs": true,
    "lib": [
      "es6",
      "dom"
    ],
    "rootDir": "src",
    "moduleResolution": "node",
    "jsxImportSource": "solid-js",
    "types": [
      "solid-js",
      "solid-js/dom"
    ]
  }
}